@namespace Pirate.UI
@using System
@using Pirate.Town
@inherits PanelComponent

<root>
    <div class="town-name">
        @Town.TownName
    </div>
</root>

@code
{
    [Property] public required Town Town { get; set; }

    protected override int BuildHash()
    {
        return HashCode.Combine(Town.TownName, Town.FishTradePrice, Town.CurrentPopulation);
    }
}